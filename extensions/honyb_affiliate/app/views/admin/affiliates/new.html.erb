<div data-hook="admin_affiliate_new_form_header">
  <%= render 'shared/error_messages', :target => @affiliate %>
  <h1><%= t(:new_affiliate) %></h1>
</div>

<div data-hook="admin_user_new_form">
  <%= form_for [:admin, @affiliate] do |f| %>
    <%= render :partial => 'form', :locals => { :f => f } %>

    <div data-hook="admin_affiliate_new_form_buttons">
      <%= render :partial => 'admin/shared/new_resource_links' %>
    </div>
  <% end %>
</div>
